GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BlocksKit" "${PODS_ROOT}/Headers/Public/DFBehaviors" "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BlocksKit" -isystem "${PODS_ROOT}/Headers/Public/DFBehaviors" -isystem "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_LDFLAGS = -ObjC -l"Pods-DFBehaviors-BlocksKit" -l"Pods-DFBehaviors-DFBehaviors" -framework "MessageUI"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods